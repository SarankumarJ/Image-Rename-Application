{
    "builds": [
        {
            "src": "image_renamer_project/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb","runtime": "Python3.8"}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "image_renamer_project/wsgi.py"
        }
    ]
}